{
  "root": "dist/user-management-system",
  "clean_urls": true,
  "routes": {
    "/**": "index.html"
  },
  "https_only": true,
  "headers": {
    "/**": {
      "Cache-Control": "no-store, no-cache, must-revalidate",
      "Strict-Transport-Security": "max-age=31536000; includeSubDomains",
      "X-Content-Type-Options": "nosniff"
    }
  }
} 